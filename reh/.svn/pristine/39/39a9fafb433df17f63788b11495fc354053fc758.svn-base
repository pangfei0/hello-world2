.td_left{width:60px;height:1px;background:#E1E1E1;}
.td_right{width:120px;height:1px;}
.tr_top{background:#E1E1E1;}
.font1{font-size:14px;color:#6C9542;font-weight:bold;}
.fontnav{margin-left:20px;margin-top:20px;}

/**********表格下载************/
.download{width:1003px;height:600px; float:left}
.more_title{ color:#AF1316; font-size:14px; font-weight:bold; margin-left:10px;}
.more_title hr{color:#DCDCDC;}

/********************底部栏*******************/
.footer{width:1005px;background:#4CA11F;padding-bottom:2px; float:left; text-align:center; margin-top:4px;}
.footnav{margin-top:20px;color:#FFF;height:30px;line-height:30px;}
.footnav a{color:#FFF;margin-left:5px;margin-right:5px;}
.footnav a:hover,.footnav a.active{color:#000;margin-left:5px;margin-right:5px;}

.copyright{color:#FFF;}
.copyright a{color:#FFF;}
.cityspread{margin-top:5px;}
.nTab1{
	float: left;
	width: 423px;
	background-position:left;
	background-repeat:no-repeat;
	margin: 0px;
}
.nTab1 .TabTitle1{
	clear: both;
	height: 30px;
	overflow: hidden;
	width: 420px;
}
.nTab1 .TabTitle1 ul{
border:0;
margin:0;
padding:0;
}
.nTab1 .TabTitle1 li{
	float: left;
	cursor: pointer;
	list-style-type: none;
	margin-right: 0px;
}
.nTab1 .TabContent1{
	width:421px;
	margin: 0px;
	overflow:hidden;
	padding-top: 6px;
	height: 250px;
}
.nTab1 .TabTitle1 .active{ width:189px; height:32px; border-bottom:#909090 thin ridge; float:left; line-height:32px; text-align:center; background:#4CA11F; color:#FFF; margin:0 auto;}
.nTab1 .TabTitle1 .normal{ width:190px; height:32px;border-bottom: #909090 thin ridge;border-left:#909090 thin ridge;float:left; line-height:32px; text-align:center; background:#CDE6E9; color:#000; margin:0 auto;}
.nTab1 .TabTitle1 .active:hover{ background:#4CA11F; color:#000; font-size:14px}
.nTab1 .TabTitle1 .normal:hover{ background:#4CA11F; color:#FFF;font-size:14px}

.nr,.nr a:link,.nr a:visited{
	float: left;
	color: #000000;
	text-decoration: none;
	line-height: 25px;
	font-size:14px;
}
.nr a:hover {
	color: #003ac0;
	text-decoration: underline;
}
.nr ul {
	width: 720px;
	margin: 0px;
	padding: 0px;
	float: left;
}
.nr ul li {
	background-image: url(../images/sx_44.gif);
	background-repeat: no-repeat;
	background-position: 4px 8px;
	float: left;
	width: 720px;
	list-style-type: none;
	padding-left: 20px;
	border-bottom-width: 1px;
	border-bottom-style: dotted;
	border-bottom-color: #CCCCCC;
}
.nr ul li span {
	color: #A85B0B;
	float: right;
	font-size:14px;
}
.info_title a:hover {
	color: #003ac0;
	text-decoration: underline;
}
.x_hei,.x_hei a:link,.x_hei a:visited {
	color: #000000;
	text-decoration: none;font-size: 14px;
}
.x_hei a:hover {
	color: #d50000;
	text-decoration: underline;font-size: 14px;
}
.time{color:#36648B;}
.pxgl{ border:1px solid #e7e7e7; border-collapse:collapse;} 
.pxgl td{ border:1px solid #e7e7e7; border-collapse:collapse;text-align:center;font-size:12px;} 
