body, dl, dt, dd, ol, ul, pre, form, field, input, blockquote, th, td, h1, h2, h3, h4, h5, h6, div { font-family: '微软雅黑', 宋体, 'microsoft yahei', Tahoma, Verdana, Arial, Helvetica, sans-serif; margin: 0; padding: 0; font-size: 12px; }
ul, ol { margin: 0px; padding: 0px; list-style: none; }
li { list-style-type: none;width:250px;overflow:hidden; }
p { margin: 0; padding: 0; color:#000; }
body, img { border: none;margin: 0; padding: 0; }
a { text-decoration: none; color:#000; }
body{
	/*background-image:url(../../images/bg.jpg);*/
	 background-repeat:no-repeat;
	 overflow-x:hidden;
	 }
.clear{clear:both; height:0px; overflow:hidden;}
.left-space{ margin-left:1px;}
.top-space{ margin-top:4px;}
.border-style{border: #909090 thin ridge;}
.main{ margin:0 auto;width:1005px;}
.underLine{ border-bottom:2px dotted; color:#E0E0E0}
/******************头部样式******************/
.header{margin:0 auto;width:1005px;}
.logo{float:left;margin-top:20px;margin-left:0px; width:220px; height:60px;}
.top_btn{float:right;font-size:12px;margin-top:3px; width:108px;height:24px;}
.top_btn a{ display:block; border:#C5C5C5 thin ridge; width:50px;height:24px; float:left; text-align:center; line-height:24px;}

.nav{ width:1005px; float:left; height:42px; margin-top:0px; background:#61A009;}
.nav ul li{width:95px; height:42px; float:left; line-height:42px; text-align:center; border-right:thin #000 ridge; }
.nav ul li a{display:block; color:#fff; font-size:12px;font-weight:bold;}
.nav ul li a:hover,.menu ul li a.active{ background:#329900; font-size:14px}

/*****************中间栏样式****************/
/*************首页*************/
.login{width:225px; height:300px;float:left;}
.login_title{ width:225px; height:32px; background:#4CA11F;border-bottom: #909090 thin ridge;}
.text{color:#FFF;line-height:32px; margin-left:10px;}
.login_detail{ width:190px; height:25px; line-height:25px; text-align:right; margin-top:22px; margin-left:4px;}
.login_btn{margin-top:55px;}
.login_btn a{width:65px; height:20px; color:#FFF;display: block; background:#6DAC13;line-height:20px; margin-left:22px;margin-right:22px; float:left; text-align:center}

.news{ width:380px;height:300px;float:left; margin-left:2px;}
.news_title_left{ width:189px; height:32px; border-bottom:#909090 thin ridge; float:left; line-height:32px; text-align:center; background:#4CA11F; color:#FFF; margin:0 auto;}
.news_title_right{ width:190px; height:32px;border-bottom: #909090 thin ridge;border-left:#909090 thin ridge;float:left; line-height:32px; text-align:center; background:#FFF; color:#000; margin:0 auto;}
.news_title_left:hover{ background:#FFF; color:#000; font-size:14px}
.news_title_right:hover{ background:#4CA11F; color:#FFF;font-size:14px}

.news_list{ margin-top:15px; float:left; }
.nlist{ width:335px; height:24px; line-height:24px; margin-left:10px;}
.nlist li{ list-style: inside disc;}
.nlist a{ float:left;}
.nlist span{ float:right; color:#8D8D8D;}

.guide{ width:390px; height:300px;float:left; margin-left:2px;}
.guide img{width:390px; height:300px;}

.adv{width:502px; height:70px; float: left; margin-top:3px;}
.adv img{width:502px; height:70px;}

.info{ width:1005px;float:left; }
.info_part{width:499px; height:300px;float:left;}
.info_title{width:499px;height:32px;line-height:32px; background:#4CA11F; }
.info_title span{color:#FFF; margin-left:5px; font-weight:bold}
.info_content{ margin-top:15px; margin-left:40px;}
.info_list{width:335px; height:24px; line-height:24px;}
.info_list li{list-style: inside disc;}
/**********招聘与求职************/
/********专场招聘*********/
.third_left{ width:120px; height:600px; float:left;}
.third_left_title{width:120px; text-align:center; margin-top:30px;}
.third_left_title span{width:120px;font-weight:bold; font-size:16px; margin-top:20px; float:left}
.third_left_title a{ color:#000;}
.third_left_title a:hover{ color:#7EBE5F;}

.third_right{ width:877px;height:600px; float:left;}
.news_more{  width:800px;margin-top:30px; float:left; padding-left:90px;}
