/********我的应聘*********/
.my_recruit{ width:880px;height:300px; float:left; }
.table_top,.table_bottom{width:500px;margin-left:40px;text-align:center; margin-top:30px;}
.table_top tr,.table_bottom tr{ height:30px;}
